{
  "extends": [
    "oclif",
    "oclif-typescript"
  ],
  "rules": {
    "quotes": ["error", "double"],
    "unicorn/prefer-node-protocol": 0,
    "object-curly-spacing": ["error", "always"],
    "no-process-exit": 0,
    "unicorn/no-process-exit": 0,
    "unicorn/empty-brace-spaces": 0,
    "semi": ["error", "always"],
    "arrow-parens": ["error", "always"],
    "node/no-missing-import": 0
  }
}
